<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.shoutsafetyhelpofferedusingtechnology">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="SHOUT"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SHOUTSafetyHelpOfferedUsingTechnology"
        tools:targetApi="31">
        <activity
            android:name=".MainPageActivity2"
            android:exported="false" />
        <activity
            android:name=".FalseAlarmMessageActivity"
            android:exported="false" />
        <activity
            android:name=".HistoryOptionsActivity"
            android:exported="false" />
        <activity
            android:name=".EditContactListActivity"
            android:exported="false" />
        <activity
            android:name=".SetUpPermissionActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>